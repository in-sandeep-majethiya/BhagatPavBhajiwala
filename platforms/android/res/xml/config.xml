<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.bhagatpavbhajiwala" version="0.1" versionCode="0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>Bhagat Pavbhajiwala</name>
    <description>
        Pav bhaji is more than a mere snack! it’s a quick meal that can be grabbed on the go – since large potions of the bhaji are made in advance and simply reheated with a few spices before serving.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Bhagat Pavbhajiwala
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
